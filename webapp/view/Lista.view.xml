<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.terraregia.Z_DEMOMIGUEL007.controller.Lista" displayBlock="true">
	<Page id="page" title="{i18n>title}">
		<content>
			<List noDataText="Drop list items here" items="{/CabeceraSet}" itemPress="onCabeceraPress" mode="Delete" delete="onDeleteCabecera">
				<items>
					<StandardListItem type="Navigation" title="{Descripcion}" description="{NombrePais}" icon="sap-icon://picture"/>
				</items>
			</List>
		</content>
		<footer>
			<Toolbar width="100%" id="toolbar1">
				<content>
					<ToolbarSpacer id="spacer0"/>
					<Button xmlns="sap.m" id="button0" activeIcon="sap-icon://add" icon="sap-icon://add" type="Emphasized" press="onCrear"/>
				</content>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>